
void setup() {
  // put your setup code here, to run once:
pinMode(D1,OUTPUT);
pinMode(D2,OUTPUT);
pinMode(D3,OUTPUT);
pinMode(D4,OUTPUT);
pinMode(D5,OUTPUT);
}

void loop() {
  // put your main code here, to run repeatedly:
digitalWrite(D1,HIGH);
delay(500);
digitalWrite(D1,LOW);
delay(500);

digitalWrite(D2,HIGH);
delay(500);
digitalWrite(D2,LOW);
delay(500);

digitalWrite(D3,HIGH);
delay(500);
digitalWrite(D3,LOW);
delay(500);

digitalWrite(D4,HIGH);
delay(500);
digitalWrite(D4,LOW);
delay(500);

digitalWrite(D5,HIGH);
delay(500);
digitalWrite(D5,LOW);
delay(500);
}
